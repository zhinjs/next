{
  "compilerOptions": {
    "target": "ES2022",
    "outDir": "./lib",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"]
}
